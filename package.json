{
	"name": "@nakanoaas/hono-stripe-webhook-middleware",
	"version": "0.0.5",
	"description": "Hono middleware for Stripe webhook signature validation",
	"author": "Nakano as a Service",
	"license": "Apache-2.0",
	"type": "module",
	"sideEffects": false,
	"publishConfig": {
		"access": "public",
		"provenance": true
	},
	"homepage": "https://github.com/nakanoasaservice/hono-stripe-webhook-middleware",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/nakanoasaservice/hono-stripe-webhook-middleware.git"
	},
	"bugs": {
		"url": "https://github.com/nakanoasaservice/hono-stripe-webhook-middleware/issues"
	},
	"keywords": [
		"hono",
		"stripe",
		"middleware",
		"cloudflare",
		"workers"
	],
	"main": "./dist/index.cjs",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.cts",
	"exports": {
		".": {
			"require": "./dist/index.cjs",
			"import": "./dist/index.mjs"
		},
		"./package.json": "./package.json"
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsdown",
		"cf-typegen": "wrangler types",
		"check-types": "tsc --noEmit",
		"check:fix": "biome check --write",
		"check": "biome check",
		"test": "vitest"
	},
	"peerDependencies": {
		"hono": ">=4.1.0",
		"stripe": ">=15.0.0"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.4.2",
		"@cloudflare/vitest-pool-workers": "^0.12.12",
		"@types/node": "^24.10.13",
		"stripe": "^20.3.1",
		"tsdown": "^0.20.3",
		"typescript": "^5.9.3",
		"vitest": "~3.2.4",
		"wrangler": "^4.65.0"
	},
	"packageManager": "pnpm@10.28.0"
}
